{
    "manifest_version": 3,
    "name": "YT Liker",
    "description": "Automatically like videos on YouTube",
    "version": "1.0.1",
    "icons": {
        "16": "/icons/16x16.png",
        "48": "/icons/48x48.png",
        "128": "/icons/128x128.png"
    },
    "content_scripts": [{
        "matches": ["https://www.youtube.com/*"],
        "js": ["/js/content.js"]
    }]
}